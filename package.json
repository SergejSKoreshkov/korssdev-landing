{
  "name": "react-webpack5-ts-boilerplate",
  "version": "0.0.1",
  "description": "Boilerplate project with React, React Router, Webpack 5, Babel, Typescript, Eslint",
  "main": "index.js",
  "repository": "https://github.com/SergejSKoreshkov/fintor.git",
  "author": "SergejSKoreshkov@gmail.com",
  "license": "MIT",
  "scripts": {
    "serve": "webpack serve --mode=development",
    "build": "webpack --mode=production",
    "lint": "eslint src/**",
    "lint:fix": "yarn lint --fix",
    "deploy:github": "NODE_ENV=github webpack --mode=production -o docs && git add . && git commit -m 'DEPLOY TO GITHUB PAGES' && git push origin main"
  },
  "dependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.22",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.0.1",
    "eslint": "^7.12.1",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.22.0",
    "file-loader": "^6.2.0",
    "path": "^0.12.7",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^5.1.1",
    "ts-loader": "^8.0.14",
    "typescript": "^4.1.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.19.0",
    "webpack-cli": "^4.4.0",
    "webpack-dev-server": "^3.11.2"
  }
}
